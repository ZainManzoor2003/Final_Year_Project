{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.gif",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "usesCleartextTraffic": true,
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-redux"
        ]
      }
    },
    "plugins": [
      "expo-video"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zain_manzoor.client"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a003e5d5-a905-47d9-9e04-103d58b85a67"
      }
    }
  }
}
